{
  "name": "ada-public-api",
  "version": "0.0.1",
  "description": "Ada public API",
  "main": "index.js",
  "author": "Gabriel Bernal <gabriel@ada-school.org>",
  "license": "PRIVATE",
  "scripts": {
    "build": "node scripts/build.js",
    "dev": "yarn build && node scripts/watch.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "compile": "tsc --noEmit",
    "lint": "eslint -c .eslintrc.json --ext .ts,.tsx ./src",
    "prepare": "husky install",
    "precommit": "concurrently --names \"lint,compile\" \"yarn lint\" \"yarn compile\"",
    "prepush": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node-fetch": "^2.6.11",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/throng": "^5.0.7",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "chokidar": "^3.6.0",
    "concurrently": "^8.2.2",
    "esbuild": "^0.20.0",
    "esbuild-node-externals": "^1.12.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-silent-reporter": "^0.5.0",
    "lint-staged": "^15.2.1",
    "mongodb-memory-server": "^9.1.6",
    "node-ts": "^6.0.1",
    "prettier": "^3.2.4",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^10.2.0",
    "mongoose": "^8.1.1",
    "node-fetch": "2.7.0",
    "swagger-autogen": "^2.23.7",
    "swagger-express-ts": "^1.1.0",
    "swagger-ui-express": "^5.0.0",
    "throng": "^5.0.0"
  }
}
