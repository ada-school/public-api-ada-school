{
  "name": "public-api-ada-school",
  "version": "1.0.0",
  "description": "My cool TypeScript app",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "compile": "ts-node build.ts && tsc",
    "dev": "nodemon server/index.ts | pino-pretty",
    "dev:debug": "nodemon --exec \"node -r ts-node/register --inspect-brk\" server/index.ts | pino-pretty",
    "lint": "eslint -c .eslintrc.js \"{server, test}/**/*.{js,ts,tsx}\" --quiet",
    "lint:fix": "eslint -c .eslintrc.js \"{server, test}/**/*.{js,ts,tsx}\" --quiet --fix",
    "test": "jest",
    "test:debug": "mocha -r ts-node/register --inspect-brk test/**/*.ts --exit",
    "prepare": "husky install"
  },
  "dependencies": {
    "body-parser": "^1.19.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.17.3",
    "express-openapi-validator": "^4.13.8",
    "mongoose": "^8.0.3",
    "pino": "^7.8.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.11",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.21",
    "@types/pino": "^7.0.4",
    "@types/shelljs": "^0.8.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.0.0",
    "chai": "^4.3.6",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^2.0.15",
    "pino-pretty": "^7.5.3",
    "prettier": "^2.5.1",
    "shelljs": "^0.8.5",
    "supertest": "^6.1.6",
    "ts-node": "^10.7.0",
    "typescript": "^4.9.5"
  },
  "author": "Carmine DiMascio <cdimascio@gmail.com> (https://github.com/cdimascio)"
}
